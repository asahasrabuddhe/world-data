{
    "name": "asahasrabuddhe/world-data",
    "type": "package",
    "description": "A package that contains data about the various countries, states and cities in the world.",
    "keywords": ["laravel", "countries", "city", "state", "province"],
    "homepage": "https://github.com/asahasrabuddhe/world-data",
    "authors": [
        {
            "name": "Ajitem Sahasrabuddhe",
            "email": "ajitem.s@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Asahasrabuddhe\\WorldData\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "symfony/console": "^3.3",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/dom-crawler": "^3.3",
        "symfony/css-selector": "^3.3"
    },
    "license": "MIT",
     "extra": {
        "laravel": {
            "providers": [
                "Asahasrabuddhe\\WorldData\\Providers\\WorldDataServiceProvider"
            ],
            "aliases": {
                "Countries": "Asahasrabuddhe\\WorldData\\Facades\\WorldDataFacade"
            }
        }
    }
}
